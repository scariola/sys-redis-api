				<munit:attributes value='#[{&#10;  "headers": {&#10;    "x-correlation-id": "s12312312312312312"&#10;  },&#10;  "listenerPath": "/api/*",&#10; "relativePath": "/api/state/639174276705",&#10;  "version": "HTTP/1.1",&#10;  "method": "GET",&#10;  "scheme": "HTTP",&#10;  "requestPath": "",&#10;  "requestUri": "",&#10;  "rawRequestPath": "/api/state/639174276705",&#10;  "localAddress": "",&#10;  "remoteAddress": ""&#10;}&#10;as Object {&#10;  class: "org.mule.extension.http.api.HttpRequestAttributes"&#10;}]' />
			</munit:set-event>
			<flow-ref doc:name="flow-ref sys-redis-api-main" doc:id="9512bab3-5e80-4ab0-b51c-06ccc7a66d86" name="sys-redis-api-main" />
		</munit:execution>
		<munit:validation >
			<munit-tools:assert-equals doc:name="Assert State" doc:id="2cd6ef29-1122-418b-8098-6c8cc09f08c1" actual="#[payload]" expected='#[output application/json&#10;---&#10;{&#10;    "state": 1&#10;}]' />
		</munit:validation>
	</munit:test>
</mule>
